<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.darlington.standardbadmintonscoreapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_bad"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_bad"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Singles"
            android:label="Badminton Score Keeper"/>
        <activity android:name=".ScoreSheet"
            android:label="Badminton Score Keeper"></activity>
    </application>

</manifest>